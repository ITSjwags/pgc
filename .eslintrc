{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": ["error", {
      "allow": [
        "warn",
        "error"
      ]
    }],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "semi": false,
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 2
      }
    ]
  }
}
